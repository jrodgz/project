doctype html
html(lang='eng')
    head
        meta(http-equiv='Content-Type', content='text/html; charset=UTF-8')
        meta(http-equiv='x-ua-compatible', content='ie=edge')
        title cs725-Memento-Archive-Collection-Visualization
        meta(name='viewport', content='width=device-width, initial-scale=1')
        link(rel='apple-touch-icon', href='apple-touch-icon.png')
        link(rel='stylesheet', href='/css/bootstrap.min.css', type='text/css')
        link(rel='stylesheet', href='/css/dashboard.css', type='text/css')
        link(rel='stylesheet', href='/css/timeline-chart.css', type='text/css')
        link(rel='stylesheet', href='/css/d3-tip.css', type='text/css')
        link(rel='stylesheet', href='/css/c3.css', type='text/css')
        link(rel='stylesheet', href='/css/table.css', type='text/css')
        script(src='/js/jquery.min.js', type='text/javascript')
        script(src='/js/d3.min.js', type='text/javascript')
        script(src='/js/c3.min.js', type='text/javascript')
        script(src='/js/d3plus.full.min.js', type='text/javascript')
        script(src='/js/dagre-d3.min.js', type='text/javascript')
        script(src='/js/lodash.min.js', type='text/javascript')
        script(src='/js/string.min.js', type='text/javascript')
        script(src='/js/serialize.js', type='text/javascript')
        script(src='/js/d3timeline.js', type='text/javascript')
        script(src='/js/d3-tip.js', type='text/javascript')
        script(src='/js/preprocess.js', type='text/javascript')
        script(src='/js/drawing.js', type='text/javascript')
        script(src='/js/slider.js', type='text/javascript')
    body.application
        nav.navbar.navbar-default.navbar-fixed-top(role='navigation')
            .container-fluid
                .navbar-header
                    p.navbar-text Memento Collection
        .container-fluid
            .row
                .col-sm-12
                    .charts
                        #box1.chart
                            .chart-wrapper
                                .chart-title Timeline
                                #chart1.slide-chart.chart-stage.timeline-chart(data-chart-desc='timeline')
                        #box2.chart
                            .chart-wrapper
                                .chart-title Word Play
                                #chart2.slide-chart.chart-stage(data-chart-desc='word spread')
                        #box3.chart
                            .chart-wrapper
                                .chart-title Tags
                                #chart3.slide-chart.chart-stage(data-chart-desc='tag histogram')
                        #box4.chart
                            .chart-wrapper
                                .chart-title Year Month Tag Domain Tree Map
                                #chart4.slide-chart.chart-stage(data-chart-desc='yearMonthTagDomainTreeMap')
                        #box5.chart
                            .chart-wrapper
                                .chart-title Domain Tag Year Month Tree Map
                                #chart5.slide-chart.chart-stage(data-chart-desc='domainTagYearMonthTreeMap')
                        #box6.chart
                            .chart-wrapper
                                .chart-title Tag Domain Year Month Tree Map
                                #chart6.slide-chart.chart-stage(data-chart-desc='yearTagDomainYearMonthTreeMap')

            .row
                .col-sm-12
                    .slideshow
                        span#prev_chart_container.move.left
                            | ←
                            span#prev_chart
                        span#next_chart_container.move.right
                            span#next_chart
                            |  →
            .row
                .col-sm-4
                    .detail-chart-wrapper
                        .chart-title Top 10 Archiving Years
                        #topTenArchivingYears.top-ten-chart.chart-stage(data-chart-desc='something')
                .col-sm-4
                    .detail-chart-wrapper
                        .chart-title Top 10 Popular Domains
                        #topTenPopularDomains.top-ten-chart.chart-stage(data-chart-desc='word spread')
                .col-sm-4
                    .detail-chart-wrapper
                        .chart-title Top 10 Common Tags
                        #topTenPopularKeywords.top-ten-chart.chart-stage(data-chart-desc='word spread')
            .row
                .col-sm-12
                    .charts2
                        each t, i in tags
                            div(class="chart2",id="bbox"+(i+1))
                                .chart-wrapper
                                    .chart-title #{t} Domain URI-R Time Graph
                                    div(class="slide-chart chart-stage",id="cchart"+(i+1),data-chart-desc='#{t} Domain URI-R Time Graph')
            .row
                .col-sm-12
                    .slideshow2
                        span#prev_chart_container2.move2.left2
                            | ←
                            span#prev_chart2
                        span#next_chart_container2.move2.right2
                            span#next_chart2
                            |  →
            .row
                .col-sm-12
                    .charts3
                        each o, i in olap
                            div(class="chart3",id="bboxx" + (i + 1))
                                .chart-wrapper
                                    .chart-title #{o.ts} Overlap Time Graph
                                    div(class="slide-chart chart-stage",id="cchartt" + (i + 1),data-chart-desc='#{o.ts} Overlap Time Graph')
            .row
                .col-sm-12
                    .slideshow3
                        span#prev_chart_container3.move3.left3
                            | ←
                            span#prev_chart3
                        span#next_chart_container3.move3.right3
                            span#next_chart3
                            |  →
        span#ruler(style='visibility: hidden; white-space: nowrap;')
        script(src='/js/bootstrap.min.js', type='text/javascript')
        script(src='/js/doVis.js')
